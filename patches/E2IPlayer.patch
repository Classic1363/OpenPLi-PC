+#!/bin/sh
+cp $1/iptvupdate/custom/tsiplayer.sh $2/iptvupdate/custom/tsiplayer.sh
+status=$?
+if [ $status -ne 0 ]; then
+	echo "Critical error. The $ 0 file can not be copied, error[$status]."
+	exit 1
+fi
+mkdir $2/tsiplayer
+mkdir $2/tsiplayer/Libs
+mkdir $2/tsiplayer/cfscrape
