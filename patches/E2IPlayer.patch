+                    self.reportHttpsError('verify', url, self.meta['pycurl_error'][1])
+                elif self.meta['pycurl_error'][0] == pycurl.E_SSL_INVALIDCERTSTATUS:
+                    self.reportHttpsError('verify', url, self.meta['pycurl_error'][1])
+        except Exception:
+            printExc()
+        return sts, data
